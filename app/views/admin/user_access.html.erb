<div class="d-flex justify-content-center mt-3 mb-3">
<h1> User Accesss </h1>
</div>
<ol class="list-group list-group-numbered">

  <% @users.each do |user| %>
    <li class="list-group-item d-flex justify-content-between align-items-start">
    <div class="ms-2 me-auto">
      <div class="fw-bold"><storng><%=user.username %></strong></div>
    </div>
    <% if user.normal? %>    
    <%= link_to "Delete", destroy_user_path(user.id), data: { turbo_method: :delete, turbo_confirm: "Are you sure?" } %>
    <% else %>
     Admin
    <% end %>
    </li>
  <% end %>
</ol>